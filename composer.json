{
  "name": "narno/phpoole-library",
  "type": "library",
  "description": "An intuitive PHP library to create a static Website.",
  "version" : "1.1.x-dev",
  "keywords": ["static", "website", "generator", "flat file", "markdown", "twig"],
  "homepage": "https://github.com/Narno/PHPoole-library",
  "license": "MIT",
  "authors": [
    {
      "name": "Arnaud Ligny",
      "email": "arnaud@ligny.org",
      "homepage": "http://narno.org",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=5.5",
    "symfony/finder": "2.*",
    "symfony/filesystem": "2.*",
    "cocur/slugify": "1.*",
    "symfony/yaml": "2.*",
    "erusev/parsedown-extra": "0.7.*",
    "twig/twig": "1.*",
    "zendframework/zend-eventmanager": "2.*",
    "myclabs/php-enum": "^1.4",
    "dflydev/dot-access-configuration": "1.0.1"
  },
  "require-dev": {
    "phpunit/phpunit": "4.8.*",
    "satooshi/php-coveralls": "dev-master"
  },
  "autoload": {
    "psr-4" : {
      "PHPoole\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PHPoole\\Test\\": "tests"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-develop": "1.1.x-dev"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
