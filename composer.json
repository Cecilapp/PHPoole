{
  "name": "phpoole/library",
  "description": "An intuitive PHP library to create a static website.",
  "keywords": ["static", "website", "generator", "flat file", "markdown", "twig"],
  "homepage": "https://github.com/Narno/PHPoole-library",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Arnaud Ligny",
      "email": "arnaud@ligny.org",
      "homepage": "http://narno.org",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^5.6|^7.1",
    "cocur/slugify": "3.0.1",
    "dflydev/dot-access-data": "1.1.0",
    "erusev/parsedown-extra": "0.7.1",
    "matthiasmullie/minify": "1.3.56",
    "myclabs/php-enum": "1.5.2",
    "symfony/filesystem": "2.8.41|3.3.13",
    "symfony/finder": "2.7.38|2.8.31|3.3.13",
    "symfony/process": "2.8.31|3.3.13",
    "symfony/yaml": "2.8.31|3.3.13",
    "twig/twig": "1.35.0"
  },
  "require-dev": {
    "phpunit/phpunit": "5.7.25",
    "sami/sami": "3.3.0",
    "php-coveralls/php-coveralls": "1.0.2"
  },
  "autoload": {
    "psr-4" : {
      "PHPoole\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PHPoole\\Test\\": "tests"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.x-dev"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
