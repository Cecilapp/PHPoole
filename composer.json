{
  "name": "narno/phpoole-library",
  "description": "An intuitive PHP library to create a static website.",
  "keywords": ["static", "website", "generator", "flat file", "markdown", "twig"],
  "homepage": "https://github.com/Narno/PHPoole-library",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Arnaud Ligny",
      "email": "arnaud@ligny.org",
      "homepage": "http://narno.org",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^5.5|^7.0",
    "cocur/slugify": "^2.0",
    "dflydev/dot-access-configuration": "^1.0",
    "erusev/parsedown-extra": "^0.7",
    "myclabs/php-enum": "^1.4",
    "symfony/filesystem": "^2.8",
    "symfony/finder": "^2.7|^2.8",
    "symfony/process": "^2.8",
    "symfony/yaml": "^2.8",
    "twig/twig": "^1.24",
    "zendframework/zend-eventmanager": "^2.6"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8",
    "satooshi/php-coveralls": "dev-master"
  },
  "autoload": {
    "psr-4" : {
      "PHPoole\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PHPoole\\Test\\": "tests"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.1.x-dev"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
